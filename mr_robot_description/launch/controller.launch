<launch>

<rosparam file="$(find mr_robot_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="mr_robot" args="Revolute 10_position_controller Revolute 9_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/mr_robot/joint_states"/>
</node>

</launch>